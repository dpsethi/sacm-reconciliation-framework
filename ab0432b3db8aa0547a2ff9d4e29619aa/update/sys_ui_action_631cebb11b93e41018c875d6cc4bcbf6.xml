        <script><![CDATA[//find if job is already running for this configuration
//var parm1 = "openstack";
var parm1 = current.name;
var parm2 = gs.getUserName();
var name = "x_tdbf2_sacm_recon.openstack.event";
var Sysevent = new global.sacmUTILglobal; //script include helper to find current state whether this job is already been processed
var returnvalue = Sysevent.findEvent(name, parm1); //function call in script include
//gs.addInfoMessage("Value in Sysevent is " + returnvalue);
gs.addInfoMessage("Value in parm1 is " + parm1);

if (returnvalue == true){ //if this event is running 
	gs.addInfoMessage("SACM Recon UTIL - This configuration is currently been processed, was sent by " + parm2);
}else{
gs.addInfoMessage("Sending this record to eventQueue for background processing   ==>" + current.sys_id);
gs.eventQueue("x_tdbf2_sacm_recon.openstack.event", current, parm1,parm2); //send to event registry which will trigger script action SPF process record
}]]></script>
